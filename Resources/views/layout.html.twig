<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Swagger UI</title>

    {% block favicons %}
        {% block al_swagger_ui_favicons %}
        {% endblock %}
    {% endblock %}

    {% block stylesheets %}
        {% block al_swagger_ui_stylesheets %}
        {% endblock %}
    {% endblock %}

</head>

<body class="swagger-section">
<div id='header'>
    <div class="swagger-ui-wrap">
        <a id="logo" href="http://swagger.io"><img class="logo__img" alt="swagger" height="30" width="30" src="{{ asset('bundles/alswaggerui/images/logo_small.png') }}" /><span class="logo__title">swagger</span></a>
        {% block al_swagger_ui_auth_form %}
        {% endblock %}
    </div>
</div>

{% block al_swagger_ui_content %}
{% endblock %}

{% block javascripts %}
    {% block al_swagger_ui_javascripts %}
    {% endblock %}
{% endblock %}

</body>
</html>
